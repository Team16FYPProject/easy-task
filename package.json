{
    "name": "easy-task",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "npm run test:unit && npm run test:e2e",
        "test:unit": "vitest run",
        "test:e2e": "npx playwright test --workers=1",
        "test:e2e:ui": "npx playwright test --ui",
        "format": "prettier --write .",
        "prepare": "husky install",
        "type-checks": "tsc --noEmit",
        "sb:types": "npx supabase gen types --lang=typescript --local > src/utils/supabase/database.types.ts",
        "db-reset": "supabase db reset && npm run sb:types"
    },
    "dependencies": {
        "@emotion/cache": "^11.13.1",
        "@emotion/react": "^11.13.0",
        "@emotion/server": "^11.11.0",
        "@emotion/styled": "^11.13.0",
        "@fontsource/roboto": "^5.0.14",
        "@mui/icons-material": "^5.16.6",
        "@mui/material": "^5.16.6",
        "@mui/material-nextjs": "^5.16.6",
        "@mui/x-charts": "^7.13.0",
        "@mui/x-data-grid": "^7.15.0",
        "@mui/x-date-pickers": "^7.17.0",
        "@supabase/ssr": "^0.4.0",
        "@supabase/supabase-js": "^2.45.0",
        "dayjs": "^1.11.13",
        "next": "^14.2.5",
        "playwright": "^1.46.1",
        "react": "^18.3.1",
        "react-big-calendar": "^1.14.1",
        "react-dom": "^18.3.1",
        "recharts": "^2.12.7",
        "resend": "^4.0.0"
    },
    "devDependencies": {
        "@playwright/test": "^1.46.1",
        "@testing-library/react": "^16.0.0",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-big-calendar": "^1.8.12",
        "@types/react-dom": "^18",
        "@types/recharts": "^1.8.29",
        "@vitejs/plugin-react": "^4.3.1",
        "autoprefixer": "^10.4.19",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.2.5",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "husky": "^8.0.0",
        "jsdom": "^24.1.1",
        "lint-staged": "^15.2.7",
        "postcss": "^8.4.40",
        "prettier": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.5",
        "tailwindcss": "^3.4.7",
        "typescript": "^5",
        "vite-tsconfig-paths": "^5.0.1",
        "vitest": "^2.0.4"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --cache --fix",
            "prettier --write ."
        ]
    }
}
